<% variables.each do |x| %>

    <% if x.get_data_structure_type == :MATRIX %>

        <%= render 'matrix', matrix_limitation: x.get_matrix_limitation %>

    <% end %>

    <% if x.get_data_structure_type == :VECTOR %>

        <%= render 'vector', vector_limitation: x.get_vector_limitation %>

    <% end %>

    <% if x.get_data_structure_type == :NUMBER %>

        <%= render 'number', number_limitation: x.get_number_limitation %>

    <% end %>

<% end %>