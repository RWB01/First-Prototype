<div class="dashboard_title_wrapper">
  <h1 class="dashboard_title">Dashboard</h1>
</div>

<% if current_user.roles.nil? || current_user.has_role(:STUDENT_ROLE) %>
    <%= render 'student' %>
<% elsif current_user.has_role(:PROFESSOR_ROLE)%>
    <%= render 'admin' %>
<% else %>
    SOMETHING WENT WRONG
<% end %>

